{
  "name": "doucheblock-for-twitter",
  "description": "Block anyone with specific keywords in their Twitter bio.",
  "version": "0.0.3",
  "homepage": "https://chrome.google.com/webstore/detail/eeledoologbepiegnccedjigjkblhmhi",
  "author": "Mobile First LLC",
  "repository": {
    "type": "git",
    "url": "https://github.com/MobileFirstLLC/doucheblock"
  },
  "scripts": {
    "start": "xt-build -e dev -w",
    "start:ff": "xt-build -e dev -w -c ff-build.json",
    "build": "xt-build -e prod",
    "build:ff": "xt-build -e prod -c ff-build.json",
    "clean": "xt-clean",
    "docs": "xt-docs",
    "test": "xt-test"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintIgnore": [
    "test/**/*"
  ],
  "devDependencies": {
    "cws-publish": "^2.0.2",
    "extension-cli": "0.11.7"
  },
  "xtbuild": {
    "manifest": "./manifests/manifest.json",
    "js_bundles": [
      {
        "name": "content",
        "src": "./src/content.js"
      },
      {
        "name": "options",
        "src": "./src/options.js"
      },
      {
        "name": "background",
        "src": "./src/background.js"
      }
    ]
  },
  "xtdocs": {
    "source": {
      "include": [
        "./.github/DOCS.md",
        "src"
      ]
    },
    "opts": {
      "destination": "./pages",
      "tutorials": "./docs"
    },
    "templates": {
      "inlineNav": true,
      "search": false,
      "analytics": {
        "ua": "UA-129118591-7",
        "domain": "https://oss.mobilefirst.me/doucheblock"
      },
      "scripts": [
        "https://buttons.github.io/buttons.js"
      ],
      "systemLogo": "./assets/img/128x128.png",
      "systemName": "DoucheBlock for Twitter",
      "systemSummary": "Block anyone with specific keywords in their Twitter bio.",
      "systemColor": "#1DA1F2",
      "methodHeadingReturns": false,
      "stylesheets": [
        "data:text/css;base64,"
      ],
      "navMembers": [
        {
          "kind": "module",
          "title": "Modules"
        },
        {
          "kind": "tutorial",
          "title": "Other Documents"
        }
      ]
    }
  }
}

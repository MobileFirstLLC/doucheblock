{
  "name": "doucheblock-for-twitter",
  "description": "Automatically block anyone with specific keywords in their profile bio",
  "version": "0.0.3",
  "homepage": "https://chrome.google.com/webstore/detail/eeledoologbepiegnccedjigjkblhmhi",
  "author": "Mobile First LLC",
  "repository": {
    "type": "git",
    "url": "https://github.com/MobileFirstLLC/doucheblock"
  },
  "scripts": {
    "start": "xt-build -e dev -w",
    "build": "xt-build -e prod",
    "clean": "xt-clean",
    "docs": "xt-docs",
    "test": "xt-test"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintIgnore": [
    "test/**/*"
  ],
  "devDependencies": {
    "extension-cli": "latest"
  },
  "xtdocs": {
    "source": {
      "include": [
        "README.md",
        "src"
      ]
    },
    "opts": {
      "destination": "./pages"
    },
    "templates": {
      "inlineNav": true,
      "search": false,
      "analytics": {
        "ua": "UA-129118591-7",
        "domain": "https://oss.mobilefirst.me/doucheblock"
      },
      "systemName": "DoucheBlock for Twitter",
      "systemSummary": "Blocks anyone with specific keywords in their profile",
      "systemColor": "#1DA1F2",
      "methodHeadingReturns": false,
      "stylesheets": [
        "data:text/css;base64,Zm9vdGVyewogICAgZGlzcGxheTpub25lIWltcG9ydGFudDsKfQ==",
        "data:text/css;base64,"
      ],
      "navMembers": [
        {
          "kind": "module",
          "title": "Modules"
        }
      ]
    }
  },
  "xtbuild": {
    "js_bundles": [
      {
        "name": "content",
        "src": "./src/content.js"
      },
      {
        "name": "options",
        "src": "./src/options.js"
      },
      {
        "name": "background",
        "src": "./src/background.js"
      }
    ]
  }
}

{
  "name": "doucheblock-for-twitter",
  "description": "Block anyone with specific keywords in their Twitter bio.",
  "version": "0.2.1",
  "homepage": "https://github.com/MobileFirstLLC/doucheblock",
  "author": "Mobile First LLC",
  "repository": {
    "type": "git",
    "url": "https://github.com/MobileFirstLLC/doucheblock"
  },
  "scripts": {
    "start": "xt-build -e dev -w -c config/chromium.json",
    "start:ff": "xt-build -e dev -w -c config/firefox.json",
    "build": "xt-build -e prod -c config/chromium.json",
    "build:ff": "xt-build -e prod -c config/firefox.json",
    "clean": "xt-clean",
    "docs": "xt-docs -c config/docs.json",
    "test": "xt-test"
  },
  "standard-version": {
    "infile": "docs/CHANGELOG.md"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintIgnore": [
    "test/**/*"
  ],
  "devDependencies": {
    "cws-publish": "^2.0.2",
    "extension-cli": "1.0.3",
    "node-sass": "^6.0.0"
  }
}
